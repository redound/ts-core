{
  "version": "1.8.9",
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "preserveConstEnums": true,
    "declaration": true,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "./lib"
  },
  "files": [
    "./src/ts-core.ts",
    "./typings/main.d.ts"
  ],
  "exclude": [
    "./typings/browser.d.ts",
    "./typings/browser",
    "./node_modules"
  ]
}
